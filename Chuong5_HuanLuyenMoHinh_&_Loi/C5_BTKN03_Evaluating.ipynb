        "id": "SeHKRbYwjczH"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def cfs_matrix(y_test, y_pred):\n",
        "  y_test = np.array(y_test)\n",
        "  y_pred = np.array(y_pred)\n",
        "  class_num = np.unique(y_test).shape[0]\n",
        "  result = np.zeros((class_num, class_num))\n",
        "  n = y_test.shape[0]\n",
        "  for i in range(n):\n",
        "    result[y_test[i], y_pred[i]] += 1\n",
        "  return result"
      ],
      "metadata": {
        "id": "idJL8R0ph4_B"
      },
